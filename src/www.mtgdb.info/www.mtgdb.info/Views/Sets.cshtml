<div style="float:left;">
    <div>
    <h4>Core</h4>
    <ul class="setlist">
    @foreach(var set in Model.GetBlock("Core"))
    {
        string imgUrl = string.Format("https://api.mtgdb.info/content/set_images/symbols/{0}_sym.png",set.Id.ToLower());

        <li><img src="@imgUrl" />
        <a href="~/sets/@set.Id">@set.Name</a>
        @if(Model.Planeswalker != null){<b>@Model.GetSetCount(@set.Id).ToString()</b>}
        Released:  @set.ReleasedAt.ToShortDateString()</li>
    }
    </ul>
